{
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": true
      }
    ]
  ],
  "plugins": ["@babel/plugin-transform-runtime", "@babel/plugin-proposal-object-rest-spread", ["babel-plugin-transform-builtin-extend", { "globals": ["Error", "Array"] }]],
  "ignore": ["node_modules"]
}
